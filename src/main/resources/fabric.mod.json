{
  "schemaVersion": 1,
  "id": "kinectminecraft",
  "version": "${version}",

  "name": "Kinect in Minecraft",
  "description": "A mod that allows you to use a Kinect to control Minecraft",
  "authors": [
    "Legadyn!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.legadyn.kinectminecraft.KinectMinecraft"
    ]
  },
  "mixins": [
    "kinectminecraft.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
